+    private DialogInterface.OnClickListener mClickListener = null;
+
+    /**
+     * M: create a instance of DeleteDialogFragment
+     * 
+     * @param single
+     *            if the number of files to be deleted is only one ?
+     * @return the instance of DeleteDialogFragment
+     */
